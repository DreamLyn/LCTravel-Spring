		<javaClientGenerator targetPackage="com.lc.travel.dao"
			targetProject="LCTravel/src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaClientGenerator>

